{
    "name": "pedhot-dev/nepotismfree-di",
    "description": "A high-performance, opinionated, strict Dependency Injection Container for PHP 8.1+. No magic, no hidden wiring.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pedhot-Dev",
            "homepage": "https://github.com/Pedhot-Dev"
        }
    ],
    "support": {
        "source": "https://github.com/Pedhot-Dev/nepotismfree-di",
        "issues": "https://github.com/Pedhot-Dev/nepotismfree-di/issues"
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "PedhotDev\\NepotismFree\\": "src/PedhotDev/NepotismFree"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PedhotDev\\NepotismFree\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "virion": {
            "spec": "3.1",
            "namespace-root": "PedhotDev\\NepotismFree"
        }
    }
}
